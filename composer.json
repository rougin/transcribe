{
    "name": "rougin/transcribe",
    "description": "Yet another language library for PHP",
    "keywords": [
        "language",
        "transcribe",
        "php"
    ],
    "homepage": "https://github.com/rougin/transcribe",
    "license": "MIT",
    "authors": [
        {
            "name": "Rougin Royce Gutib",
            "email": "rougingutib@gmail.com",
            "homepage": "http://rougin.github.io",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Rougin\\Transcribe\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rougin\\Transcribe\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.3-dev"
        }
    }
}